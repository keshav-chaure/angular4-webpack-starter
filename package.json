{
  "name": "angular4-webpack-seed",
  "version": "0.0.1",
  "description": "Angular4 Webpack Starter",
  "main": "index.ts",
  "scripts": {
    "serve": "webpack-dev-server --config config/webpack.dev.js",
    "build:dev": "npm run clean && webpack --config config/webpack.dev.js --progress",
    "build:prod": "npm run clean && webpack --config config/webpack.prod.js --progress",
    "start": "npm run serve",
    "clean": "rimraf dist/ src/ngfactory lib",
    "test": "karma start config/karma.conf.js",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run protractor ./config/protractor.conf.js",
    "pree2e": "npm run webdriver:update -- --standalone",
    "protractor": "protractor",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "ci": "npm run lint && npm test && npm run e2e",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "tslint": "tslint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/nzick/angular4-webpack-seed/"
  },
  "license": "MIT",
  "dependencies": {
    "core-js": "2.4.1",
    "ng2-translate": "^5.0.0"
  },
  "devDependencies": {
    "@angular/common": "4.0.0-rc.6",
    "@angular/compiler": "4.0.0-rc.6",
    "@angular/compiler-cli": "4.0.0-rc.6",
    "@angular/core": "4.0.0-rc.6",
    "@angular/forms": "4.0.0-rc.6",
    "@angular/http": "4.0.0-rc.6",
    "@angular/platform-browser": "4.0.0-rc.6",
    "@angular/platform-browser-dynamic": "4.0.0-rc.6",
    "@angular/platform-server": "4.0.0-rc.6",
    "@angular/router": "4.0.0-rc.6",
    "@angularclass/hmr": "^1.2.2",
    "@angularclass/hmr-loader": "^3.0.2",
    "@ngtools/webpack": "^1.2.14",
    "@types/jasmine": "2.5.45",
    "@types/node": "^7.0.10",
    "@types/node-sass": "3.10.32",
    "add-asset-html-webpack-plugin": "^1.0.2",
    "angular2-template-loader": "^0.6.2",
    "autoprefixer": "^6.7.6",
    "awesome-typescript-loader": "^3.0.8",
    "codelyzer": "2.0.0-beta.4",
